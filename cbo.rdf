    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/3/Babbage2#childOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/3/Babbage2#childOf">
