        <rdfs:comment xml:lang="fr">Une personne qui est compétente dans le domaine scientifique.</rdfs:comment>
        <rdfs:label xml:lang="fr">estLeDomaineScientifiqueDe</rdfs:label>
        <rdfs:label xml:lang="en">isScientificDomainOf</rdfs:label>
    </owl:ObjectProperty>
    


