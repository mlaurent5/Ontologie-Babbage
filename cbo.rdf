<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/marcl/ontologies/2023/3/Babbage2/"
     xml:base="http://www.semanticweb.org/marcl/ontologies/2023/3/Babbage2/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:cbo="http://www.semanticweb.org/marcl/ontologies/2023/3/cbo#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:cbo1="http://www.semanticweb.org/marcl/ontologies/2023/cbo#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:Babbage2="http://www.semanticweb.org/marcl/ontologies/2023/3/Babbage2#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo">
        <dc:description xml:lang="en">An ontology about the correspondence of Charles Babbage.</dc:description>
        <rdfs:comment xml:lang="fr">Une ontologie sur la correspondance de Charles Babbage.</rdfs:comment>
        <rdfs:label xml:lang="en">cbo</rdfs:label>
        <rdfs:label xml:lang="fr">ocb</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/isPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isPartOf">
        <owl:equivalentProperty rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#belongsToSubject"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/references">
        <owl:equivalentProperty rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#references"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/subject">
        <owl:equivalentProperty rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasSubject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#belongsToSubject -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#belongsToSubject">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Subject"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Subject"/>
        <rdfs:comment xml:lang="en">Define which subjects are encompassing this one.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Définit le ou les sujets qui englobent celui-ci.</rdfs:comment>
        <rdfs:label xml:lang="fr">appartientAuSujet</rdfs:label>
        <rdfs:label xml:lang="en">belongsToSubject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#childOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#childOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#relatedTo"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#parentOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:comment xml:lang="fr">Les personnes indentifiées dont un individu est le descendant.</rdfs:comment>
        <rdfs:comment xml:lang="en">The identified persons from which someone descends.</rdfs:comment>
        <rdfs:label xml:lang="en">childOf</rdfs:label>
        <rdfs:label xml:lang="fr">enfantDe</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasAddress -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasAddress">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Address"/>
        <rdfs:comment xml:lang="fr">L&apos;adresse qui correspond à un lieu.</rdfs:comment>
        <rdfs:comment xml:lang="en">The adress which corresponds to a place.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourAddresse</rdfs:label>
        <rdfs:label xml:lang="en">hasAddress</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasArchivedAdress -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasArchivedAdress">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isArchiveAdressOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#ArchivePlace"/>
        <rdfs:comment xml:lang="fr">L&apos;adresse à laquelle une lettre est archivée aujourd&apos;hui.</rdfs:comment>
        <rdfs:comment xml:lang="en">The adress at which a letter is archived today.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourAdresseDArchivage</rdfs:label>
        <rdfs:label xml:lang="en">hasArchivedAddress</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasBirthPlace -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasBirthPlace">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isBirthPlaceOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:comment xml:lang="fr">Le lieu de naissance d&apos;une ou plusieurs personnes.</rdfs:comment>
        <rdfs:comment xml:lang="en">The birth place of one or more persons.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourlieuDeNaissance</rdfs:label>
        <rdfs:label xml:lang="en">hasBirthPlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasCorrespondent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasCorrespondent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:comment xml:lang="en">A person that was either the writer or the receiver of the letter.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une personne concernée par la lettre, qui l&apos;a reçue ou écrite.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourCorrespondant</rdfs:label>
        <rdfs:label xml:lang="en">hasCorrespondent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasDestinationAddress -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasDestinationAddress">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isDestinationAdressOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Address"/>
        <rdfs:comment xml:lang="fr">L&apos;adresse à laquelle la lettre a été envoyée.</rdfs:comment>
        <rdfs:comment xml:lang="en">The adress to which the letter was sent.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourAdresseDeDestination</rdfs:label>
        <rdfs:label xml:lang="en">hasDestinationAddress</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasDuration -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Inhabitation"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Membership"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Practice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Duration"/>
        <rdfs:comment xml:lang="fr">La durée d&apos;une occurence, que ce soit l&apos;habitation d&apos;un lieu, la pratique d&apos;une discipline, ou l&apos;appartenance à une institution.</rdfs:comment>
        <rdfs:comment xml:lang="en">The duration of an event, be it the inhabitation of a localisation, the practice in a field, or the membership to an institution.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourDurée</rdfs:label>
        <rdfs:label xml:lang="en">hasDuration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasInstitution -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasInstitution">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isInstitutionOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Membership"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Institution"/>
        <rdfs:comment xml:lang="fr">L&apos;institution à laquelle cette personne appartient.</rdfs:comment>
        <rdfs:comment xml:lang="en">The organization to which this person belongs.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourInstitution</rdfs:label>
        <rdfs:label xml:lang="en">hasInstitution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasLocation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasLocation">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isLocationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Inhabitation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:comment xml:lang="fr">La localisation d&apos;une habitation donnée.</rdfs:comment>
        <rdfs:comment xml:lang="en">The location corresponding to an habitation.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourLocalisation</rdfs:label>
        <rdfs:label xml:lang="en">hasLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasMembership -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasMembership">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isMembershipOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Membership"/>
        <rdfs:comment xml:lang="fr">L&apos;appartenance à une institution pour une personne donnée, qui peut n&apos;être effective que pour un epériode donnée.</rdfs:comment>
        <rdfs:comment xml:lang="en">The membership to an istitution by a given person, that may only be true for a defined span of time.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourAdhésion</rdfs:label>
        <rdfs:label xml:lang="en">hasMembership</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPlaceOfDeath -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPlaceOfDeath">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isPlaceOfDeathOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:comment xml:lang="fr">Le lieu de mort d&apos;une personne.</rdfs:comment>
        <rdfs:comment xml:lang="en">The location at which a person died.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourLieuDeDécès</rdfs:label>
        <rdfs:label xml:lang="en">hasPlaceOfDeath</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPractice -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPractice">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isPracticeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Practice"/>
        <rdfs:comment xml:lang="en">The practice in a given field by a giiven person, that may or may not be maintained only for a given span of time.</rdfs:comment>
        <rdfs:comment>Une pratique d&apos;un domaine par la personne, qui peut avoir lieu seulement pour une durée définie.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourPratique</rdfs:label>
        <rdfs:label xml:lang="en">hasPractice</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPracticePlace -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasPracticePlace">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Practice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:comment xml:lang="en">A pratice takes place at a location.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une pratique se déroule dans un lieu.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourlieuDePratique</rdfs:label>
        <rdfs:label xml:lang="en">hasPracticePlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasReply -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasReply">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isResponseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:comment xml:lang="fr">La lettre qui est la réponse de celle-ci.</rdfs:comment>
        <rdfs:comment xml:lang="en">The letter which replied to this one.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourRéponse</rdfs:label>
        <rdfs:label xml:lang="en">hasReply</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasScientificField -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasScientificField">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isScientificDomainOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Practice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Field"/>
        <rdfs:comment xml:lang="en">A practice is linked to one or more scientific fields.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une pratique est liée à un ou plusieurs domaines scientifiques.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourDomaineScientifique</rdfs:label>
        <rdfs:label xml:lang="en">hasScientificField</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasSentAddress -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasSentAddress">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isSendingAddressOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Address"/>
        <rdfs:comment xml:lang="fr">L&apos;adresse depuis laquelle la lettre a été envoyée.</rdfs:comment>
        <rdfs:comment xml:lang="en">The adress from which the letter was sent.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourAdresseDEnvoi</rdfs:label>
        <rdfs:label xml:lang="en">hasSendingAddress</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasSubject -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#hasSubject">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isSubjectOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Subject"/>
        <rdfs:comment xml:lang="en">A letter can have a single subject, multiple ones, or none in particular.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une lettre peut avoir un sujet, plusieurs, ou aucun en particulier.</rdfs:comment>
        <rdfs:label xml:lang="fr">aPourSujet</rdfs:label>
        <rdfs:label xml:lang="en">hasSubject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#isArchiveAdressOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isArchiveAdressOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#ArchivePlace"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:comment xml:lang="en">An archive can contain multiple letters.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Un lieu d&apos;archivage peut entreposer de multiples lettres.</rdfs:comment>
        <rdfs:label xml:lang="fr">estLAdresseDArchivageDe</rdfs:label>
        <rdfs:label xml:lang="en">isArchiveAdressOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#isBirthPlaceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isBirthPlaceOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Place"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Person"/>
        <rdfs:comment xml:lang="en">A person that was born at this place.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une personne qui est née en ce lieu.</rdfs:comment>
        <rdfs:label xml:lang="fr">estLeLieuDeNaissanceDe</rdfs:label>
        <rdfs:label xml:lang="en">isBirthPlaceOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#isDestinationAdressOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marcl/ontologies/2023/cbo#isDestinationAdressOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Address"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marcl/ontologies/2023/cbo#Letter"/>
        <rdfs:comment xml:lang="en">A letter that was sent to the given address.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Une lettre qui a l&apos;adresse pour destination.</rdfs:comment>
        <rdfs:label xml:lang="fr">estLAdresseDeDestinationDe</rdfs:label>
        <rdfs:label xml:lang="en">isDestinationAdressOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marcl/ontologies/2023/cbo#isInstitutionOf -->

